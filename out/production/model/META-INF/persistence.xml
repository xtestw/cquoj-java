<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">

    <persistence-unit name="NewPersistenceUnit">
        <provider>org.hibernate.ejb.HibernatePersistence</provider>
        <class>com.cquoj.model.Category</class>
        <class>com.cquoj.model.Challenge</class>
        <class>com.cquoj.model.Config</class>
        <class>com.cquoj.model.Contest</class>
        <class>com.cquoj.model.ContestClarify</class>
        <class>com.cquoj.model.ContestProblem</class>
        <class>com.cquoj.model.ContestUser</class>
        <class>com.cquoj.model.Discuss</class>
        <class>com.cquoj.model.Mail</class>
        <class>com.cquoj.model.News</class>
        <class>com.cquoj.model.Ojinfo</class>
        <class>com.cquoj.model.Problem</class>
        <class>com.cquoj.model.ProblemCategory</class>
        <class>com.cquoj.model.Ranklist</class>
        <class>com.cquoj.model.ReplayStatus</class>
        <class>com.cquoj.model.Solver</class>
        <class>com.cquoj.model.Solverlist</class>
        <class>com.cquoj.model.Status</class>
        <class>com.cquoj.model.TimeBbs</class>
        <class>com.cquoj.model.User</class>
        <class>com.cquoj.model.Usertag</class>
        <class>com.cquoj.model.Vurl</class>
        <properties>
            <property name="hibernate.connection.url" value="jdbc:mysql://localhost:3306/cquoj"/>
            <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
            <property name="hibernate.connection.username"/>
            <property name="hibernate.connection.password"/>
            <property name="hibernate.archive.autodetection" value="class"/>
            <property name="hibernate.show_sql" value="true"/>
            <property name="hibernate.format_sql" value="true"/>
            <property name="hbm2ddl.auto" value="update"/>
        </properties>
    </persistence-unit>
</persistence>
